    "husky": "^7.0.4",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "should": "^13.2.3",
    "sinon": "^9.2.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
