    "start:file": "npm run build:file",
    "ts": "nodemon --exec ./node_modules/.bin/ts-node -- ",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "build:file": "nodemon --exec ./node_modules/.bin/ts-node --",
